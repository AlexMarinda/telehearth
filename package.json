{
  "name": "new-nodejs-app",
  "version": "1.0.0",
  "description": "NodeJs REST API",
  "main": "app.js",
  "scripts": {
    "test": "jest --collect-coverage",
    "dev": "nodemon --exec babel-node ./src/app.js",
    "clean:dist": "rm -rf dist && mkdir dist",
    "build-server": "babel -d ./dist ./src",
    "build": "cross-env babel src --out-dir dist --ignore '**/__tests__,**/__mocks__'",
    "start": "node ./dist/app.js",
    "migrate": "babel-node src/database/migrations/migration"
  
  },
  "keywords": [
    "NodeJS",
    "generate-node-project"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@hapi/joi": "^17.1.1",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.8",
    "celebrate": "^11.0.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.0",
    "dev-rw-phone": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.3",
    "express-mung": "^0.5.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "numeral": "^2.0.6",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sharp": "^0.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "^24.8.0",
    "nodemon": "^2.0.0",
    "supertest": "^4.0.2"
  }
